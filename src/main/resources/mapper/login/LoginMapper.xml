<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.casb.casbadmin.mapper.login.LoginMapper">

    <select id="login" resultType="java.lang.Boolean">
        select count(1) from user_info where user_id = #{userId} and password = #{password}
    </select>
    <select id="list" resultType="java.lang.Object">
        select * from goods_info
    </select>
</mapper>